{
  "name": "react-testgen",
  "version": "1.0.4",
  "description": "A generator that scaffolds unit tests for React components",
  "keywords": [
    "react",
    "typescript",
    "jest",
    "unit test",
    "unit test generator",
    "scaffold",
    "scaffolding",
    "boilerplate"
  ],
  "author": "Dustin Holm",
  "license": "MIT",
  "homepage": "https://github.com/DustinHolm/react-testgen",
  "repository": {
    "type": "git",
    "url": "https://github.com/DustinHolm/react-testgen.git"
  },
  "bugs": {
    "url": "https://github.com/DustinHolm/react-testgen/issues"
  },
  "bin": {
